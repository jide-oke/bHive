{
  "manifest_version": 3,
  "name": "Plus One",
  "version": "1.9",
  "description": "View JSON file entries in a user-friendly format.",
  "permissions": ["tabs", "storage", "activeTab", "scripting"],
  "host_permissions": ["http://localhost:3001/*", "https://getclever.lightning.force.com/*"],
  "action": {
    "default_title": "Open Viewer",
    "default_icon": "current_images/icon.png"
  },
  "icons": {
    "16": "current_images/icon.png",
    "48": "current_images/icon.png",
    "128": "current_images/icon.png"
  },
  "background": {
    "service_worker": "background/background.js"
  }
}