{
    "manifest_version": 3,
    "name": "JSON Viewer Extension",
    "version": "1.0",
    "description": "View JSON file entries in a user-friendly format.",
    "permissions": ["tabs"],
    "action": {
      "default_title": "Open Viewer"
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["Database1.json"],
        "matches": ["<all_urls>"]
      }
    ]
  }