{
  "manifest_version": 3,
  "name": "JSON Viewer Extension",
  "version": "1.0",
  "description": "View JSON file entries in a user-friendly format.",
  "permissions": ["tabs"],
  "host_permissions": ["http://localhost:3000/*"],
  "action": {
    "default_title": "Open Viewer"
  },
  "background": {
    "service_worker": "background.js"
  }
}